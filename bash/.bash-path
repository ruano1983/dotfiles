### PATH

if [ -d "$HOME/.local/bin" ] ;
  then PATH="$HOME/.local/bin:$PATH"
fi

if [ -d "$HOME/.cargo/bin" ] ;
  then PATH="$HOME/.cargo/bin:$PATH"
fi

if [ -d "$HOME/Aplicaciones" ] ;
  then PATH="$HOME/Aplicaciones:$PATH"
fi

if [ -d "$HOME/Aplicaciones/Telegram" ] ;
  then PATH="$HOME/Aplicaciones/Telegram:$PATH"
fi

if [ -d "$HOME/scripts" ] ;
  then PATH="$HOME/scripts:$PATH"
fi

if [ -d "/opt/android-tools" ] ;
  then PATH="/opt/android-tools:$PATH"
fi
